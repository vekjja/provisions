{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "blue": "#00b3ff",
    "gray": "#888888",
    "pink": "#e7357c"
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "leading_diamond": "  \ue0b2",
          "trailing_diamond": "\ue0b0",
          "template": "\ue235 {{ .Venv }} ",
          "foreground": "#100e23",
          "background": "#906cff",
          "type": "python",
          "style": "diamond"
        },
        {
          "leading_diamond": "<transparent,background>\ue0b0</>",
          "trailing_diamond": "\ue0b0",
          "template": " {{ .Full }} ",
          "foreground": "#000000",
          "background": "#ffea4d",
          "type": "python",
          "style": "diamond",
          "newline": true
        }
      ],
      "newline": true
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "leading_diamond": "  \ue0b2",
          "trailing_diamond": "\ue0b0",
          "template": "\udb82\udc33 {{ .Context }} ",
          "foreground": "#000000",
          "background": "#a85cff",
          "type": "kubectl",
          "style": "diamond"
        },
        {
          "options": {
            "context_aliases": {
              "CLUSTER NAME": "\ued97"
            },
            "max_width": 3,
            "min_width": 10
          },
          "leading_diamond": "<transparent,background>\ue0b0</>",
          "trailing_diamond": "\ue0b0",
          "template": "{{ if .Namespace }} {{ .Namespace }}{{ end }} ",
          "foreground": "#000000",
          "background": "#dcff30",
          "type": "kubectl",
          "style": "diamond"
        }
      ],
      "newline": true
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "options": {
            "folder_icon": "\u2026",
            "style": "agnoster_full"
          },
          "leading_diamond": "\u256d\u2500\ue0b2",
          "trailing_diamond": "\ue0b0",
          "template": "\udb81\udf70 {{ .Path }} ",
          "foreground": "#000000",
          "powerline_symbol": "\ue0b0",
          "background": "p:blue",
          "type": "path",
          "style": "diamond",
          "background_templates": [
            "{{ if eq .Env.USER \"root\" }}p:pink{{ end }}"
          ]
        },
        {
          "options": {
            "branch_gone_icon": "\ueab8",
            "branch_icon": "\ue725 ",
            "branch_identical_icon": "\uf444",
            "branch_max_length": 25,
            "fetch_stash_count": true,
            "fetch_status": true
          },
          "leading_diamond": "<transparent,background>\ue0b0</>",
          "trailing_diamond": "\ue0b0",
          "template": "{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} <transparent>\ue0b1</> <#121318>\uf044 {{ .Working.String }}</>{{ end }}{{ if .Staging.Changed }} <transparent>\ue0b1</> <#121318>\uf046 {{ .Staging.String }}</>{{ end }}{{ if gt .StashCount 0 }} <transparent>\ue0b1</> <#121318>\ueb4b {{ .StashCount }}</>{{ end }} ",
          "foreground": "#000000",
          "background": "#35e7b7",
          "type": "git",
          "style": "diamond",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#e7357c{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#54433a{{ end }}",
            "{{ if gt .Ahead 0 }}#ffaf00{{ end }}",
            "{{ if gt .Behind 0 }}#424242{{ end }}"
          ]
        },
        {
          "options": {
            "always_enabled": true,
            "folder_icon": "\u2026",
            "folder_separator_icon": " <transparent>\ue0b1</> ",
            "max_depth": 3,
            "style": "austin"
          },
          "template": " {{ .FormattedMs }} ",
          "foreground": "#f2f3f8",
          "powerline_symbol": "\ue0b0",
          "background": "#424242",
          "type": "executiontime",
          "style": "powerline"
        }
      ],
      "newline": true
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "template": "\u2570\uea9c ",
          "foreground": "p:blue",
          "type": "text",
          "style": "plain",
          "foreground_templates": [
            "{{ if eq .Env.USER \"root\" }}p:pink{{ end }}"
          ]
        },
        {
          "template": "{{ if .SSHSession }}\udb80\udd5f{{ else }}\uf109{{ end }} ",
          "foreground": "p:gray",
          "type": "session",
          "style": "plain",
          "foreground_templates": [
            "{{ if eq .Env.USER \"root\" }}p:blue{{ end }}",
            "{{ if .SSHSession }}p:pink{{ end }}"
          ]
        },
        {
          "template": "{{.UserName}}",
          "foreground": "p:blue",
          "type": "session",
          "style": "plain",
          "foreground_templates": [
            "{{ if eq .UserName \"root\" }}p:pink{{ end }}"
          ]
        },
        {
          "options": {
            "kali": "\uf327"
          },
          "template": " {{ .Icon }} ",
          "foreground": "p:gray",
          "type": "os",
          "style": "plain",
          "foreground_templates": [
            "{{ if .Env.SSH_CONNECTION }}p:pink{{ end }}",
            "{{ if eq .Env.USER \"root\" }}p:blue{{ end }}"
          ]
        },
        {
          "template": "{{ .HostName }}:",
          "foreground": "p:blue",
          "type": "session",
          "style": "plain",
          "foreground_templates": [
            "{{ if eq .UserName \"root\" }}p:pink{{ end }}"
          ]
        }
      ],
      "newline": true
    }
  ],
  "version": 4,
  "final_space": true
}