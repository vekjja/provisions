          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "editorMode": "code",
          "expr": "100 *\nsum by (namespace, pod, container, node) (\n  rate(container_cpu_usage_seconds_total{\n    namespace=~\"$namespace\",\n    pod=~\".+\",\n    container=~\".+\",\n    container!=\"POD\",\n    image=~\".+\"\n  }[$__rate_interval])\n)\n/\non (node) group_left\nmax by (node) (machine_cpu_cores)",
          "hide": false,
          "instant": false,
          "legendFormat": "{{namespace}}: {{container}}",
          "range": true,
          "refId": "Pod CPU Usage"
        }
      ],
      "title": "CPU Usage",
      "type": "timeseries"
